  @@index([productId])
}

model ProductVariant {
  id                     String      @id @default(uuid()) @postgresql.Uuid
  product                Product?    @relation("ProductVariant_product", fields: [productId], references: [id])
  productId              String?     @map("product") @postgresql.Uuid
  price                  Int
  pieces                 Int?
