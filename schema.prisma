  updatedAt DateTime?
}

model ToppingOption {
  id         String     @id @default(uuid()) @postgresql.Uuid
  topping    Topping?   @relation("ToppingOption_topping", fields: [toppingId], references: [id])
  toppingId  String?    @map("topping") @postgresql.Uuid
